{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "additionalProperties": true,
    "properties": {
        "enable_private_network_middleware": {
            "description": "Applied the private network middleware",
            "title": "Private Network Authentication",
            "type": "boolean"
        },
        "enabled": {
            "description": "Boolean to enable this chart in the cluster",
            "title": "Enable",
            "type": "boolean"
        },
        "hostname": {
            "description": "The hostname",
            "minLength": 1,
            "title": "Hostname",
            "type": "string"
        },
        "kubee_internal": {
            "properties": {
                "install_cross_dependency": {
                    "type": "boolean"
                }
            },
            "type": "object"
        },
        "namespace": {
            "description": "The installation namespace",
            "minLength": 1,
            "title": "Namespace",
            "type": "string"
        }
    },
    "required": [
        "enabled",
        "namespace",
        "hostname",
        "enable_private_network_middleware"
    ],
    "type": "object"
}
